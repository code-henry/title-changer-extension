{
    "manifest_version": 3,
    "name": "Change Tab Title",
    "version": "1.0",
    "permissions": ["contextMenus", "tabs", "scripting", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon.svg",
      "48": "icon.svg",
      "128": "icon.svg"
    },
    "commands": {
      "change-title": {
        "suggested_key": {
          "default": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y"
        },
        "description": "Change the tab title"
      }
    }
  }